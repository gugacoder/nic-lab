    "# 🔍 VALIDAÇÃO E TESTE DE BUSCA\n",
    "print(\"🔍 Validando armazenamento e testando busca...\")\n",
    "\n",
    "from qdrant_utils import search_similar_vectors\n",
    "import random\n",
    "import numpy as np\n",
    "\n",
    "# Verificar estado final da collection\n",
    "print(\"\\n📊 Verificando estado final da collection...\")\n",
    "final_collection_info = get_collection_info(\n",
    "    qdrant_config[\"url\"],\n",
    "    qdrant_config[\"collection_name\"],\n",
    "    qdrant_config[\"api_key\"]\n",
    ")\n",
    "\n",
    "if final_collection_info:\n",
    "    print(f\"   📦 Collection: {final_collection_info['collection_name']}\")\n",
    "    print(f\"   📊 Vetores finais: {final_collection_info['vectors_count']:,}\")\n",
    "    print(f\"   📄 Pontos finais: {final_collection_info['points_count']:,}\")\n",
    "    print(f\"   📐 Dimensões: {final_collection_info['config']['vector_size']}\")\n",
    "    \n",
    "    vectors_stored = final_collection_info['vectors_count']\n",
    "    points_stored = final_collection_info['points_count']\n",
    "    \n",
    "    if vectors_stored > 0:\n",
    "        print(f\"   ✅ Collection populada com sucesso\")\n",
    "    else:\n",
    "        print(f\"   ⚠️ Collection vazia - possível problema na inserção\")\n",
    "else:\n",
    "    print(\"❌ Não foi possível verificar estado da collection\")\n",
    "    vectors_stored = 0\n",
    "    points_stored = 0\n",
    "\n",
    "# Teste de busca se há vetores armazenados\n",
    "search_test_results = []\n",
    "\n",
    "if vectors_stored > 0 and qdrant_config[\"test_search_after_insert\"]:\n",
    "    print(\"\\n🔍 Executando testes de busca...\")\n",
    "    \n",
    "    # Criar vetor de teste aleatório\n",
    "    test_vector = np.random.rand(qdrant_config[\"vector_size\"]).tolist()\n",
    "    \n",
    "    # Normalizar vetor de teste\n",
    "    magnitude = np.linalg.norm(test_vector)\n",
    "    if magnitude > 0:\n",
    "        test_vector = (np.array(test_vector) / magnitude).tolist()\n",
    "    \n",
    "    try:\n",
    "        # Teste de busca simples\n",
    "        search_results = search_similar_vectors(\n",
    "            qdrant_config[\"url\"],\n",
    "            qdrant_config[\"collection_name\"],\n",
    "            qdrant_config[\"api_key\"],\n",
    "            test_vector,\n",
    "            limit=5\n",
    "        )\n",
    "        \n",
    "        if search_results:\n",
    "            print(f\"   ✅ Busca funcionando: {len(search_results)} resultados encontrados\")\n",
    "            \n",
    "            # Mostrar detalhes dos primeiros resultados\n",
    "            for i, result in enumerate(search_results[:3], 1):\n",
    "                score = result.get(\"score\", 0)\n",
    "                result_id = result.get(\"id\", \"unknown\")\n",
